{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "WebSocketOption": {
    "RequestUrlKey": "/websocket", //页面路径
    "KeepAliveInterval": 60, //服务端保持ping客户端秒数
    "ReceiveBufferSize": 4096, //接收数据的缓存大小
    "WebSocketMessageType": 1 //Text:0,Binary:1
  }
}
